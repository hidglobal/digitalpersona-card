{
  "name": "@digitalpersona/card",
  "globals": [
    "Card.WebApi"
  ],
  "amdName": "Card.WebApi",
  "version": "1.0.1",
  "description": "HID DigitalPersona Card API Library",
  "license": "MIT",
  "author": "Andrey Bronnikov <andrey.bronnikov@hidglobal.com>",
  "contributors": [
    "HID Global Corporation"
  ],
  "repository": {
    "type": "git",
    "url": "github:hidglobal/digitalpersona-card#v1"
  },
  "publishConfig": {
    "access": "public",
    "tag": "deprecated"
  },
  "keywords": [
    "DigitalPersona",
    "card",
    "authentication",
    "identification"
  ],
  "files": [
    "dist/",
    "docs/usage/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/card.sdk.d.ts",
      "unpkg": "./dist/card.sdk.min.js",
      "browser": "./dist/card.sdk.js"
    }
  },
  "-browserslist": [
    "defaults and fully supports es6-module",
    "edge >= 17",
    "not iOS > 0",
    "not Android > 0"
  ],
  "dependencies": {
    "tslib": "^2.7.0"
  },
  "volta": {
    "node": "20.17.0"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "tsc": "tsc -p ./tsconfig.json",
    "minify": "terser ./dist/card.sdk.js -o ./dist/card.sdk.min.js --source-map \"content='./dist/card.sdk.js.map'\"",
    "build": "npm run clean && tsc && npm run minify",
    "publish:pre": "npm publish --tag pre --access=restricted"
  },
  "devDependencies": {
    "@digitalpersona/websdk": "^1.0.1",
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "esbuild": "^0.25.2",
    "eslint": "^9.9.1",
    "eslint-plugin-compat": "^6.0.0",
    "rimraf": "^6.0.1",
    "terser": "^5.39.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "peerDependencies": {
    "@digitalpersona/websdk": "^1.0.1"
  }
}
